Items.filter(\r\n        cartItem => cartItem.product_id !== curElem.product_id\r\n      )\r\n      state.cartItems = nextCartitems\r\n      return {\r\n        ...state, totalItems: state.totalItems - curElem.quantity, totalAmount: state.totalAmount - curElem.mrp * curElem.quantity,\r\n      };\r\n      break;\r\n\r\n    case 'productFilter':\r\n      let search = action.data;\r\n      //console.log(action.data);\r\n      return {\r\n        ...state, search: search,\r\n      };\r\n      break;\r\n    default:\r\n      return state\r\n  }\r\n\r\n}","D:\\xampp\\htdocs\\ReactJsEcommerce\\src\\containers\\HeaderContainer.js",[],"D:\\xampp\\htdocs\\ReactJsEcommerce\\src\\components\\Header.js",["150","151","152","153","154","155","156","157","158","159","160","161","162","163","164","165","166","167","168","169"],"D:\\xampp\\htdocs\\ReactJsEcommerce\\src\\App.js",[],"D:\\xampp\\htdocs\\ReactJsEcommerce\\src\\components\\Contact.js",["170","171"],"// import './App.css';\r\nimport React from 'react';\r\nimport { ToastContainer, toast } from 'react-toastify';\r\n\r\nconst Contact = () => {\r\n\r\n    return (\r\n\r\n        <div>\r\n            <section id=\"contact-us\" className=\"contact-us section\">\r\n                <div className=\"container\">\r\n                    <div className=\"contact-head\">\r\n                        <div className=\"row\">\r\n                            <div className=\"col-lg-8 col-12\">\r\n                                <div className=\"form-main\">\r\n                                    <div className=\"title\">\r\n                                        <h4>Get in touch</h4>\r\n                                        <h3>Write us a message</h3>\r\n                                    </div>\r\n                                    <form className=\"form\" method=\"post\" action=\"mail/mail.php\">\r\n                                        <div className=\"row\">\r\n                                            <div className=\"col-lg-6 c